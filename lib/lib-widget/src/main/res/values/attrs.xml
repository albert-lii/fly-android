<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 自定义标题栏 -->
    <declare-styleable name="fly_TitleBar">
        <!--标题栏文字描述-->
        <attr name="tb_title" format="string" />
        <!--标题栏文字大小-->
        <attr name="tb_titleSize" format="dimension" />
        <!--标题栏文字颜色-->
        <attr name="tb_titleColor" format="color|reference" />
        <!--标题栏文字是否加粗-->
        <attr name="tb_titleBold" format="boolean" />
        <!--左侧Padding-->
        <attr name="tb_leftPadding" format="dimension" />
        <!--左侧ICON-->
        <attr name="tb_leftIcon" format="reference" />
        <!--左侧ICON高度-->
        <attr name="tb_leftIconHight" format="dimension" />
        <!--是否显示左侧ICON-->
        <attr name="tb_showLeftIcon" format="boolean" />
        <!--左侧文字描述-->
        <attr name="tb_leftText" format="string" />
        <!--左侧文字大小-->
        <attr name="tb_leftTextSize" format="dimension" />
        <!--左侧文字颜色-->
        <attr name="tb_leftTextColor" format="color|reference" />
        <!--左侧ICON距离文字的间距-->
        <attr name="tb_leftTextLeftMargin" format="dimension" />
        <!--是否显示左侧文字-->
        <attr name="tb_showLeftText" format="boolean" />
        <!--是否显示左侧功能区域-->
        <attr name="tb_showLeftFunc" format="boolean" />
        <!--右侧Padding-->
        <attr name="tb_rightPadding" format="dimension" />
        <!--右侧ICON-->
        <attr name="tb_rightIcon" format="reference" />
        <!--右侧ICON高度-->
        <attr name="tb_rightIconHight" format="dimension" />
        <!--是否显示右侧ICON-->
        <attr name="tb_showRightIcon" format="boolean" />
        <!--右侧文字描述-->
        <attr name="tb_rightText" format="string" />
        <!--右侧文字大小-->
        <attr name="tb_rightTextSize" format="dimension" />
        <!--右侧文字颜色-->
        <attr name="tb_rightTextColor" format="color|reference" />
        <!--右侧ICON距离文字的间距-->
        <attr name="tb_rightTextLeftMargin" format="dimension" />
        <!--是否显示右侧文字-->
        <attr name="tb_showRightText" format="boolean" />
        <!--是否显示右侧功能区域-->
        <attr name="tb_showRightFunc" format="boolean" />
        <!--底部分割线的高度-->
        <attr name="tb_dividerHeight" format="dimension" />
        <!--底部分割线的颜色-->
        <attr name="tb_dividerColor" format="color|reference" />
        <!--是否显示底部分割线-->
        <attr name="tb_showDivider" format="boolean" />
        <!--标题栏高度，不包含分割线和状态栏高度-->
        <attr name="tb_barHeight" format="dimension" />
        <!--是否添加状态栏的高度-->
        <attr name="tb_addStatusBarHeight" format="boolean" />
    </declare-styleable>

    <!-- 自定义进度格控件 -->
    <declare-styleable name="fly_ProgressCellView">
        <attr name="pc_totalCount" format="integer" />
        <attr name="pc_accentCount" format="integer" />
        <attr name="pc_primaryColor" format="color|reference" />
        <attr name="pc_accentColor" format="color|reference" />
        <attr name="pc_cellWidth" format="dimension" />
        <attr name="pc_cellSpace" format="dimension" />
        <attr name="pc_reverse" format="boolean" />
        <attr name="pc_oritenation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

    <!-- 带阴影的Layout -->
    <declare-styleable name="fly_ShadowLayout">
        <attr name="sl_cornerRadius" format="dimension" />
        <attr name="sl_shadowRadius" format="dimension" />
        <attr name="sl_shadowColor" format="color" />
        <attr name="sl_dx" format="dimension" />
        <attr name="sl_dy" format="dimension" />
    </declare-styleable>

    <!-- 曲线图 -->
    <declare-styleable name="fly_CurveChart">
        <!--X轴的宽-->
        <attr name="cc_xAxisWidth" format="dimension" />
        <!--X轴的颜色-->
        <attr name="cc_xAxisColor" format="color|reference" />
        <!--X轴的文字大小-->
        <attr name="cc_xAxisTextSize" format="dimension" />
        <!--X轴的文字颜色-->
        <attr name="cc_xAxisTextColor" format="color|reference" />
        <!--X轴的整行刻度文字距离X轴尾端的右间距-->
        <attr name="cc_xAxisTextRowRightMargin" format="dimension" />
        <!--X轴的整行刻度文字距离原点的左间距-->
        <attr name="cc_xAxisTextRowLeftMargin" format="dimension" />
        <!--X轴的整行刻度文字距离原点的上间距-->
        <attr name="cc_xAxisTextRowTopMargin" format="dimension" />
        <!--Y轴的宽-->
        <attr name="cc_yAxisWidth" format="dimension" />
        <!--Y轴的颜色-->
        <attr name="cc_yAxisColor" format="color|reference" />
        <!--Y轴的文字大小-->
        <attr name="cc_yAxisTextSize" format="dimension" />
        <!--Y轴的文字颜色-->
        <attr name="cc_yAxisTextColor" format="color|reference" />
        <!--Y轴的整列刻度文字距离Y轴顶点的上间距-->
        <attr name="cc_yAxisTextColTopMargin" format="dimension" />
        <!--Y轴的整列刻度文字距离原点的右间距-->
        <attr name="cc_yAxisTextColRightMargin" format="dimension" />
        <!--是否显示Y轴线-->
        <attr name="cc_showYLine" format="boolean" />
        <!--X轴刻度线的宽-->
        <attr name="cc_xMarkLineWidth" format="dimension" />
        <!--X轴刻度线的颜色-->
        <attr name="cc_xMarkLineColor" format="color|reference" />
        <!--Y轴刻度线的宽-->
        <attr name="cc_yMarkLineWidth" format="dimension" />
        <!--Y轴刻度线的颜色-->
        <attr name="cc_yMarkLineColor" format="color|reference" />
        <!--是否显示X轴刻度线-->
        <attr name="cc_showXMarkLine" format="boolean" />
        <!--是否显示Y轴刻度线-->
        <attr name="cc_showYMarkLine" format="boolean" />
        <!--曲线的粗细-->
        <attr name="cc_curveWidth" format="dimension" />
        <!--曲线的颜色-->
        <attr name="cc_curveColor" format="color|reference" />
    </declare-styleable>

    <!--多功能输入框-->
    <declare-styleable name="fly_MultiEditText">
        <!--是否显示标题-->
        <attr name="met_show_title" format="boolean" />
        <!--标题-->
        <attr name="met_title" format="string|reference" />
        <!--标题文字颜色-->
        <attr name="met_title_textColor" format="color|reference" />
        <!--标题文字大小-->
        <attr name="met_title_textSize" format="dimension" />
        <!--标题是否加粗-->
        <attr name="met_title_bold" format="boolean" />
        <!--标题与输入框之间的间距-->
        <attr name="met_title_marginBottom" format="dimension" />
        <!--输入框获取焦点时，标题的文字颜色-->
        <attr name="met_title_focusTextColor" format="color|reference" />
        <!--输入框的内容-->
        <attr name="met_input" format="string|reference" />
        <!--输入框的ellipsize-->
        <attr name="met_input_ellipsize" format="string">
            <enum name="start" value="1" />
            <enum name="end" value="2" />
            <enum name="middle" value="3" />
            <enum name="marquee" value="4" />
        </attr>
        <!--输入内容的长度-->
        <attr name="met_input_maxLength" format="integer" />
        <!--输入框内容最多显示几行，当 met_input_type=textMultiLine 有效-->
        <attr name="met_input_maxLines" format="integer" />
        <!--输入框的提示内容-->
        <attr name="met_input_hint" format="string|reference" />
        <!--输入框的提示文字颜色-->
        <attr name="met_input_hintColor" format="color|reference" />
        <!--输入框文字颜色-->
        <attr name="met_input_textColor" format="color|reference" />
        <!--输入框文字大小-->
        <attr name="met_input_textSize" format="dimension" />
        <!--输入框左内边距-->
        <attr name="met_input_paddingLeft" format="dimension" />
        <!--输入框右右内边距-->
        <attr name="met_input_paddingRight" format="dimension" />
        <!--输入框上上内边距-->
        <attr name="met_input_paddingTop" format="dimension" />
        <!--输入框下下内边距-->
        <attr name="met_input_paddingBottom" format="dimension" />
        <!--输入框的右外边距-->
        <attr name="met_input_marginRight" format="dimension" />
        <!--输入框的高度-->
        <attr name="met_input_height" format="dimension" />
        <!--输入框的背景-->
        <attr name="met_input_focusBg" format="color|reference" />
        <attr name="met_input_unFocusBg" format="color|reference" />
        <!--输入框所在行的容器背景-->
        <attr name="met_input_rowFocusBg" format="color|reference" />
        <attr name="met_input_rowUnFocusBg" format="color|reference" />
        <!--输入框所在行的容器内边距-->
        <attr name="met_input_rowPaddingLeft" format="dimension" />
        <attr name="met_input_rowPaddingRight" format="dimension" />
        <attr name="met_input_rowPaddingTop" format="dimension" />
        <attr name="met_input_rowPaddingBottom" format="dimension" />
        <!--输入框的输入类型-->
        <attr name="met_input_type" format="string">
            <!--不设置-->
            <enum name="none" value="0" />
            <!--文本输入-->
            <enum name="text" value="1" />
            <!--文本密码-->
            <enum name="textPassword" value="2" />
            <!--数字输入-->
            <enum name="number" value="3" />
            <!--带小数点的数字-->
            <enum name="numberDecimal" value="4" />
            <!--数字密码-->
            <enum name="numberPassword" value="5" />
            <!--在开头带正号或者负号的数字输入-->
            <enum name="numberSigned" value="6" />
            <!--多行输入-->
            <enum name="textMultiLine" value="7" />
        </attr>
        <!--是否显示密码可见icon-->
        <attr name="met_show_eye" format="boolean" />
        <!--密码可见icon-->
        <attr name="met_eye_open_icon" format="reference" />
        <!--密码不可见icon-->
        <attr name="met_eye_close_icon" format="reference" />
        <!--密码可见icon的宽度-->
        <attr name="met_eye_width" format="dimension" />
        <!--密码可见icon的高度-->
        <attr name="met_eye_height" format="dimension" />
        <!--密码是否可见-->
        <attr name="met_password_visible" format="boolean" />
        <!--是否显示文本清除icon-->
        <attr name="met_show_clear" format="boolean" />
        <!--文本清除icon-->
        <attr name="met_clear_icon" format="reference" />
        <!--文本清除icon的宽度-->
        <attr name="met_clear_width" format="dimension" />
        <!--文本清除icon的高度-->
        <attr name="met_clear_height" format="dimension" />
        <!--是否显示左侧icon-->
        <attr name="met_show_leftIcon" format="boolean" />
        <!--左侧icon-->
        <attr name="met_leftIcon" format="reference" />
        <!--左侧icon宽度-->
        <attr name="met_leftIcon_width" format="dimension" />
        <!--左侧icon高度-->
        <attr name="met_leftIcon_height" format="dimension" />
        <!--左侧icon的右外间距-->
        <attr name="met_leftIcon_marginRight" format="dimension" />
        <!--是否显示右侧按钮-->
        <attr name="met_show_rBtn" format="boolean" />
        <!--右侧按钮宽度-->
        <attr name="met_rBtn_width" format="dimension" />
        <!--右侧按钮高度-->
        <attr name="met_rBtn_height" format="dimension" />
        <!--右侧按钮文字-->
        <attr name="met_rBtn_text" format="string|reference" />
        <!--右侧按钮文字颜色-->
        <attr name="met_rBtn_textColor" format="color|reference" />
        <!--右侧按钮文字大小-->
        <attr name="met_rBtn_textSize" format="dimension" />
        <!--右侧按钮文字是否加粗-->
        <attr name="met_rBtn_bold" format="boolean" />
        <!--右侧按钮背景-->
        <attr name="met_rBtn_bg" format="color|reference" />
        <!--右侧按钮与输入框的间距-->
        <attr name="met_rBtn_marginLeft" format="dimension" />
        <!--右侧按钮的左内间距-->
        <attr name="met_rBtn_paddingLeft" format="dimension" />
        <!--右侧按钮的上内间距-->
        <attr name="met_rBtn_paddingTop" format="dimension" />
        <!--右侧按钮的右内间距-->
        <attr name="met_rBtn_paddingRight" format="dimension" />
        <!--右侧按钮的下内间距-->
        <attr name="met_rBtn_paddingBottom" format="dimension" />
        <!--右侧按钮的下内间距-->
        <attr name="met_rBtn_marginBottom" format="dimension" />
        <!--是否保留错误提示的高度空间-->
        <attr name="met_reserve_errorHeight" format="boolean" />
        <!--错误提示文本-->
        <attr name="met_error" format="string|reference" />
        <!--错误提示文字大小-->
        <attr name="met_error_textSize" format="dimension" />
        <!--错误提示文字颜色-->
        <attr name="met_error_textColor" format="dimension" />
        <!--错误提示文字外边距-->
        <attr name="met_error_marginLeft" format="dimension" />
        <attr name="met_error_marginTop" format="dimension" />
        <attr name="met_error_marginRight" format="dimension" />
        <!--错误提示的高度-->
        <attr name="met_error_height" format="dimension" />
    </declare-styleable>

    <!--Banner指示器-->
    <declare-styleable name="fly_Indicator">
        <!--活动指示器的颜色-->
        <attr name="indicator_activeColor" format="color" />
        <!--非活动指示器的颜色-->
        <attr name="indicator_inactiveColor" format="color" />
        <!--非活动指示器的缩放因子，相对于活动指示器尺寸而言，范围0f-1f-->
        <attr name="indicator_inactiveScale" format="float" />
        <!--指示器之间的间隔-->
        <attr name="indicator_gap" format="dimension" />
    </declare-styleable>
</resources>