<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--=========================================================================================-->
    <!--公用属性-->
    <!--=========================================================================================-->

    <!--标题相关-->
    <attr name="fu_title" format="string" />
    <attr name="fu_titleSize" format="dimension" />
    <attr name="fu_titleColor" format="color|reference" />

    <!--Radius相关-->
    <attr name="fu_radius" format="dimension" />
    <attr name="fu_topLeftRadius" format="dimension" />
    <attr name="fu_topRightRadius" format="dimension" />
    <attr name="fu_bottomLeftRadius" format="dimension" />
    <attr name="fu_bottomRightRadius" format="dimension" />

    <!--Border相关-->
    <attr name="fu_borderWidth" format="dimension" />
    <attr name="fu_pressedBorderColor" format="color|reference" />
    <attr name="fu_disabledBorderColor" format="color|reference" />
    <attr name="fu_normalBorderColor" format="color|reference" />

    <!--方向-->
    <attr name="fu_oritenation" format="enum">
        <enum name="horizontal" value="0" />
        <enum name="vertical" value="1" />
    </attr>


    <attr name="fu_checked" format="boolean" />


    <!--左侧ICON相关-->
    <attr name="fu_leftIcon" format="reference" />
    <attr name="fu_leftIconWidth" format="dimension" />
    <attr name="fu_leftIconHeight" format="dimension" />

    <!--=========================================================================================-->
    <!--自定义控件属性-->
    <!--=========================================================================================-->

    <!-- 自定义标题栏 -->
    <declare-styleable name="fly_uikit_TitleBar">
        <!--标题栏文字描述-->
        <attr name="fu_title" />
        <!--标题栏文字大小-->
        <attr name="fu_titleSize" />
        <!--标题栏文字颜色-->
        <attr name="fu_titleColor" />
        <!--标题栏文字是否加粗-->
        <attr name="fu_titleBold" format="boolean" />
        <!--左侧功能区Padding Left-->
        <attr name="fu_leftPaddingL" format="dimension" />
        <!--左侧功能区Padding Right-->
        <attr name="fu_leftPaddingR" format="dimension" />
        <!--左侧ICON-->
        <attr name="fu_leftIcon" />
        <!--左侧ICON宽度-->
        <attr name="fu_leftIconWidth" />
        <!--左侧ICON高度-->
        <attr name="fu_leftIconHeight" />
        <!--左侧文字描述-->
        <attr name="fu_leftText" format="string" />
        <!--左侧文字大小-->
        <attr name="fu_leftTextSize" format="dimension" />
        <!--左侧文字颜色-->
        <attr name="fu_leftTextColor" format="color|reference" />
        <!--左侧ICON距离文字的间距-->
        <attr name="fu_leftTextLeftMargin" format="dimension" />
        <!--右侧功能区Padding Left-->
        <attr name="fu_rightPaddingL" format="dimension" />
        <!--右侧功能区Padding Right-->
        <attr name="fu_rightPaddingR" format="dimension" />
        <!--右侧ICON-->
        <attr name="fu_rightIcon" format="reference" />
        <!--右侧ICON宽度-->
        <attr name="fu_rightIconWidth" format="dimension" />
        <!--右侧ICON高度-->
        <attr name="fu_rightIconHight" format="dimension" />
        <!--右侧文字描述-->
        <attr name="fu_rightText" format="string" />
        <!--右侧文字大小-->
        <attr name="fu_rightTextSize" format="dimension" />
        <!--右侧文字颜色-->
        <attr name="fu_rightTextColor" format="color|reference" />
        <!--右侧ICON距离文字的间距-->
        <attr name="fu_rightTextLeftMargin" format="dimension" />
        <!--底部分割线的高度-->
        <attr name="fu_dividerHeight" format="dimension" />
        <!--底部分割线的颜色-->
        <attr name="fu_dividerColor" format="color|reference" />
        <!--标题栏高度，不包含分割线和状态栏高度-->
        <attr name="fu_barHeight" format="dimension" />
        <!--是否添加状态栏的高度-->
        <attr name="fu_addStatusBarHeight" format="boolean" />
    </declare-styleable>

    <!-- 自定义进度格控件 -->
    <declare-styleable name="fly_uikit_LatticeView">
        <attr name="fu_totalCount" format="integer" />
        <attr name="fu_accentCount" format="integer" />
        <attr name="fu_primaryColor" format="color|reference" />
        <attr name="fu_accentColor" format="color|reference" />
        <attr name="fu_cellWidth" format="dimension" />
        <attr name="fu_cellSpace" format="dimension" />
        <attr name="fu_reverse" format="boolean" />
        <attr name="fu_oritenation" />
    </declare-styleable>

    <!-- 带阴影的Layout -->
    <declare-styleable name="fly_uikit_ShadowLayout">
        <!--阴影的圆角弧度-->
        <attr name="fu_cornerRadius" format="dimension" />
        <!--阴影的半径-->
        <attr name="fu_shadowRadius" format="dimension" />
        <!--阴影的颜色，注意：颜色必须带有透明度的值-->
        <attr name="fu_shadowColor" format="color" />
        <!--阴影X轴的偏移量-->
        <attr name="fu_dx" format="dimension" />
        <!--阴影Y轴的偏移量-->
        <attr name="fu_dy" format="dimension" />
    </declare-styleable>

    <!-- 曲线图 -->
    <declare-styleable name="fly_uikit_CurveChart">
        <!--X轴的宽-->
        <attr name="fu_xAxisWidth" format="dimension" />
        <!--X轴的颜色-->
        <attr name="fu_xAxisColor" format="color|reference" />
        <!--X轴的文字大小-->
        <attr name="fu_xAxisTextSize" format="dimension" />
        <!--X轴的文字颜色-->
        <attr name="fu_xAxisTextColor" format="color|reference" />
        <!--X轴的整行刻度文字距离X轴尾端的右间距-->
        <attr name="fu_xAxisTextRowRightMargin" format="dimension" />
        <!--X轴的整行刻度文字距离原点的左间距-->
        <attr name="fu_xAxisTextRowLeftMargin" format="dimension" />
        <!--X轴的整行刻度文字距离原点的上间距-->
        <attr name="fu_xAxisTextRowTopMargin" format="dimension" />
        <!--Y轴的宽-->
        <attr name="fu_yAxisWidth" format="dimension" />
        <!--Y轴的颜色-->
        <attr name="fu_yAxisColor" format="color|reference" />
        <!--Y轴的文字大小-->
        <attr name="fu_yAxisTextSize" format="dimension" />
        <!--Y轴的文字颜色-->
        <attr name="fu_yAxisTextColor" format="color|reference" />
        <!--Y轴的整列刻度文字距离Y轴顶点的上间距-->
        <attr name="fu_yAxisTextColTopMargin" format="dimension" />
        <!--Y轴的整列刻度文字距离原点的右间距-->
        <attr name="fu_yAxisTextColRightMargin" format="dimension" />
        <!--是否显示Y轴线-->
        <attr name="fu_showYLine" format="boolean" />
        <!--X轴刻度线的宽-->
        <attr name="fu_xMarkLineWidth" format="dimension" />
        <!--X轴刻度线的颜色-->
        <attr name="fu_xMarkLineColor" format="color|reference" />
        <!--Y轴刻度线的宽-->
        <attr name="fu_yMarkLineWidth" format="dimension" />
        <!--Y轴刻度线的颜色-->
        <attr name="fu_yMarkLineColor" format="color|reference" />
        <!--是否显示X轴刻度线-->
        <attr name="fu_showXMarkLine" format="boolean" />
        <!--是否显示Y轴刻度线-->
        <attr name="fu_showYMarkLine" format="boolean" />
        <!--曲线的粗细-->
        <attr name="fu_curveWidth" format="dimension" />
        <!--曲线的颜色-->
        <attr name="fu_curveColor" format="color|reference" />
    </declare-styleable>

    <!--多功能输入框-->
    <declare-styleable name="fly_uikit_MultiEditText">
        <!--输入框的内容-->
        <attr name="fu_input" format="string|reference" />
        <!--输入框的ellipsize-->
        <attr name="fu_inputEllipsize" format="string">
            <enum name="start" value="1" />
            <enum name="end" value="2" />
            <enum name="middle" value="3" />
            <enum name="marquee" value="4" />
        </attr>
        <!--输入内容的长度-->
        <attr name="fu_inputMaxLength" format="integer" />
        <!--输入框内容最多显示几行，当 met_input_type=textMultiLine 有效-->
        <attr name="fu_inputMaxLines" format="integer" />
        <!--输入框的提示内容-->
        <attr name="fu_inputHint" format="string|reference" />
        <!--输入框的提示文字颜色-->
        <attr name="fu_inputHintColor" format="color|reference" />
        <!--输入框文字颜色-->
        <attr name="fu_inputTextColor" format="color|reference" />
        <!--输入框文字大小-->
        <attr name="fu_inputTextSize" format="dimension" />
        <!--输入框左内边距-->
        <attr name="fu_inputPaddingL" format="dimension" />
        <!--输入框右右内边距-->
        <attr name="fu_inputPaddingR" format="dimension" />
        <!--输入框上上内边距-->
        <attr name="fu_inputPaddingT" format="dimension" />
        <!--输入框下下内边距-->
        <attr name="fu_inputPaddingB" format="dimension" />
        <!--输入框的右外边距-->
        <attr name="fu_inputMarginR" format="dimension" />
        <!--输入框的高度-->
        <attr name="fu_inputHeight" format="dimension" />
        <!--输入框的背景-->
        <attr name="fu_inputFocusBg" format="color|reference" />
        <attr name="fu_inputUnFocusBg" format="color|reference" />
        <!--输入框的输入类型-->
        <attr name="fu_inputType" format="string">
            <!--不设置-->
            <enum name="none" value="0" />
            <!--文本输入-->
            <enum name="text" value="1" />
            <!--文本密码-->
            <enum name="textPassword" value="2" />
            <!--数字输入-->
            <enum name="number" value="3" />
            <!--带小数点的数字-->
            <enum name="numberDecimal" value="4" />
            <!--数字密码-->
            <enum name="numberPassword" value="5" />
            <!--在开头带正号或者负号的数字输入-->
            <enum name="numberSigned" value="6" />
            <!--多行输入-->
            <enum name="textMultiLine" value="7" />
        </attr>
        <!--密码是否可见-->
        <attr name="fu_pwdVisible" format="boolean" />
        <!--是否显示密码可见icon-->
        <attr name="fu_showPwdIcon" format="boolean" />
        <!--密码可见icon-->
        <attr name="fu_pwdIcon_open" format="reference" />
        <!--密码不可见icon-->
        <attr name="fu_pwdIcon_close" format="reference" />
        <!--密码可见icon的宽度-->
        <attr name="fu_pwdIconWidth" format="dimension" />
        <!--密码可见icon的高度-->
        <attr name="fu_pwdIconHeight" format="dimension" />
        <!--是否显示文本清除icon-->
        <attr name="fu_showClearIcon" format="boolean" />
        <!--文本清除icon-->
        <attr name="fu_clearIcon" format="reference" />
        <!--文本清除icon的宽度-->
        <attr name="fu_clearIconWidth" format="dimension" />
        <!--文本清除icon的高度-->
        <attr name="fu_clearIconHeight" format="dimension" />
        <!--是否显示左侧icon-->
        <attr name="fu_showLeftIcon" format="boolean" />
        <!--左侧icon-->
        <attr name="fu_leftIcon" />
        <!--左侧icon宽度-->
        <attr name="fu_leftIconWidth" />
        <!--左侧icon高度-->
        <attr name="fu_leftIconHeight" />
        <!--左侧icon的右外间距-->
        <attr name="fu_leftIconMarginR" format="dimension" />
        <!--是否保留错误提示的高度空间-->
        <attr name="fu_reserve_errorHeight" format="boolean" />
        <!--错误提示文本-->
        <attr name="fu_error" format="string|reference" />
        <!--错误提示文字大小-->
        <attr name="fu_errorTextSize" format="dimension" />
        <!--错误提示文字颜色-->
        <attr name="fu_errorTextColor" format="dimension" />
        <!--错误提示文字外边距-->
        <attr name="fu_errorMarginL" format="dimension" />
        <attr name="fu_errorMarginT" format="dimension" />
        <attr name="fu_errorMarginR" format="dimension" />
        <!--错误提示的高度-->
        <attr name="fu_errorHeight" format="dimension" />
    </declare-styleable>

    <!--Banner指示器-->
    <declare-styleable name="fly_uikit_Indicator">
        <!--活动指示器的颜色-->
        <attr name="fu_activeColor" format="color|reference" />
        <!--非活动指示器的颜色-->
        <attr name="fu_inactiveColor" format="color|reference" />
        <!--非活动指示器的缩放因子，相对于活动指示器尺寸而言，范围0f-1f-->
        <attr name="fu_inactiveScale" format="float" />
        <!--指示器之间的间隔-->
        <attr name="fu_gap" format="dimension" />
    </declare-styleable>

    <!--圆角View-->
    <declare-styleable name="fly_uikit_RoundView">
        <attr name="fu_radius" />
        <attr name="fu_topLeftRadius" />
        <attr name="fu_topRightRadius" />
        <attr name="fu_bottomLeftRadius" />
        <attr name="fu_bottomRightRadius" />
        <attr name="fu_normalBgColor" format="color|reference" />
        <attr name="fu_normalTextColor" format="color|reference" />
        <attr name="fu_normalBorderColor" />
        <attr name="fu_pressedBgColor" format="color|reference" />
        <attr name="fu_pressedBorderColor" />
        <attr name="fu_disabledBgColor" format="color|reference" />
        <attr name="fu_disabledTextColor" format="color|reference" />
        <attr name="fu_disabledBorderColor" />
        <attr name="fu_borderWidth" />
    </declare-styleable>

    <!--Radio ImageView-->
    <declare-styleable name="fly_uikit_RadioImageView">
        <attr name="fu_checked" />
        <attr name="fu_checkSrc" format="reference" />
        <attr name="fu_uncheckSrc" format="reference" />
    </declare-styleable>
</resources>